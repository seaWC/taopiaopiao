.profile-header{
  background: #ff4d64;
  height: 300rpx;
  text-align: center;
  position: relative;
  width: 100%;
}

.profile-img{
  width: 200rpx;
  height: 200rpx;
  display: block;
  overflow: hidden;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  transform: translate(-50%, 10rpx);
}

.profile-name{
  font-size: 36rpx;
  color: #fff;
  position: absolute;
  left: 50%;
  transform: translate(-50%, 220rpx);
}
.v-row{
  border-bottom: 1px #f1f1f1 solid;
  margin: 20rpx 20rpx;
}
.v-col{
  width:100%;
  text-align: center;
  margin: 20rpx 0;
}
.profile-m1{
  width: 50rpx;
  height:50rpx;
  margin:10rpx 0;
}
.profile-text1{
  width:100%;
  line-height: 24rpx;
  display:block;
  font-size: 24rpx;
}
.profile-cell{
  width:100%;
  border-bottom: 1px #f1f1f1 solid;
  margin: 20rpx 20rpx;
}
.v-cell{
  background: none;
  border: none;
  padding:0;
}
